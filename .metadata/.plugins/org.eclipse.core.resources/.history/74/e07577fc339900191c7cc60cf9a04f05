package br.com.questoes;

public class Questao12 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for (int i = 1; i < 11; i++) {
			System.out.print(" ");
			for (int j = 1; j < 11; j++) {
				if(i == j) {
					System.out.println("x-x");
				}
				System.out.println(i + "-" + j + " ");
			}
		}
	}
}
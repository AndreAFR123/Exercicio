package OrientacaoObjetos;

public class Secretaria extends Funcionario{

	private double numRamal;

	public double getNumRamal() {
		return numRamal;
	}

	public void setNumRamal(double numRamal) {
		this.numRamal = numRamal;
	}
	
	public void consultaDado() {
		System.out.printf("Nome: %s \nSalário: %.1f \nBonificação: %.1f \nNumero Ramal: %.1f", 
				getNome(), getSalario(), getBonifica(), getNumRamal());
	}
}

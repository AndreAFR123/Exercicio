package br.com.questoes;

import br.com.util.Teclado;

public class Questao03 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int num = Teclado.lerInt("Digite um Numero: ");
		
		for(int i =0; i < 10; i++) {
			num = i * num;
			System.out.println(num);
		}
		
	}
}
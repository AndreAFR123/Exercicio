public class Questao11 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a, b, result = 0;
		a = Teclado.lerInt("Digite a base: ");
		b = Teclado.lerInt("Digite o expoente: ");
		
		for(int i = 0; i < b; i++) {
			b *= a;
		}
		System.out.println(a);
	}
}
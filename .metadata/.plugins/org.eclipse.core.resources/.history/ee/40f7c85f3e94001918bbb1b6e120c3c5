package OrientacaoObjetos;

public class Exec {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ContaBanco cb1 = new ContaBanco();
		cb1.numCB = 150;
		cb1.saldo = 1000;
		System.out.println("Limite: " + cb1.limite);
		System.out.println("Numero: " + cb1.numCB);
		System.out.println("Saldo: " + cb1.saldo);
		System.out.println("-----------------------");
		
		ContaBanco cb2 = new ContaBanco();
		cb2.numCB = 150;
		cb2.saldo = 1000;
		System.out.println("Limite: " + cb2.limite);
		System.out.println("Numero: " + cb2.numCB);
		System.out.println("Saldo: " + cb2.saldo);
		System.out.println("-----------------------");
		
		AgenciaBanco ab1 = new AgenciaBanco();
		ab1.num = 123;
		System.out.println("NumeroAgencia: " + ab1.num);
		System.out.println("-----------------------");
		
		AgenciaBanco ab2 = new AgenciaBanco();
		ab2.num = 123;
		System.out.println("NumeroAgencia: " + ab2.num);
		System.out.println("-----------------------");
		
		CartaoCredito cc1 = new CartaoCredito();
		cc1.num = 1;
		cc1.validade = 16092002;
		cc1.cliente.idC = 123;
		cc1.cliente.nome = "Andre";
		System.out.println("Número Cartao: " + cc1.num);
		System.out.println("Validade: " + cc1.validade);
		System.out.println("ID Cliente: " + cc1.cliente.idC);
		System.out.println("Nome Cliente: " + cc1.cliente.nome);
		System.out.println("-----------------------");
		
		CartaoCredito cc2 = new CartaoCredito();
		cc2.num = 1;
		cc2.validade = 16092002;
		cc2.cliente.idC = 404;
		cc2.cliente.nome = "Felipe";
		System.out.println("Numero Cartao: " + cc2.num);
		System.out.println("Validade: " + cc2.validade);
		System.out.println("ID Cliente: " + cc2.cliente.idC);
		System.out.println("Nome Cliente: " + cc2.cliente.nome);
		System.out.println("-----------------------");
		
		Cliente c2 = new Cliente();
		c2.idC = 2;
		c2.nome = "Felipe";
		System.out.println("ID: " + c2.idC);
		System.out.println("Nome: " + c2.nome);
		System.out.println("-----------------------");
		
		Cliente c1 = new Cliente();
		c1.idC = 1;
		c1.nome = "Andre";
		System.out.println("ID: " + c2.idC);
		System.out.println("Nome: " + c2.nome);
		System.out.println("-----------------------");
	}
}
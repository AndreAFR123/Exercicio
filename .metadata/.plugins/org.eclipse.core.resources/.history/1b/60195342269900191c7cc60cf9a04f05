package br.com.questoes;

import br.com.util.Teclado;

public class Questao09 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int num = Teclado.lerInt("Digite um numero: ");
		
		for(int i = 0; i < num; i++) {
			if(num % 3 == 0 && num % 5 == 0 ) {
				System.out.println(num);
			}
		}
		
	}
}